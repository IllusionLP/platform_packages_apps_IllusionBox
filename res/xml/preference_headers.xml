<preference-headers xmlns:android="http://schemas.android.com/apk/res/android" >

    <header
        android:title="@string/header_category_ib" />

    <header
        android:id="@+id/interface_settings"
        android:fragment="com.illusion.box.fragments.sb.InterfaceSettings"
        android:icon="@drawable/illusion_statusbar"
        android:title="@string/interface_settings_title" />

    <header
        android:id="@+id/button_settings"
        android:fragment="com.illusion.box.fragments.navigation.NavigationSettings"
        android:title="@string/button_settings_title"
        android:icon="@drawable/illusion_navigation"/>

    <header
        android:key="notification_drawer_settings"
        android:title="@string/qs_settings_title"
        android:fragment="com.illusion.box.fragments.qs.QsSettings"
        android:icon="@drawable/illusion_notification" />

    <header
        android:id="@+id/custom_settings"
        android:fragment="com.illusion.box.fragments.recents.RecentSettings"
        android:title="@string/recents_panel_category"
        android:icon="@drawable/illusion_recents" />

    <header
        android:key="lock_clock"
        android:title="@string/lock_clock_title"
        android:icon="@drawable/illusion_clock">
        <intent android:action="android.intent.action.MAIN"
            android:targetPackage="com.cyanogenmod.lockclock"
            android:targetClass="com.cyanogenmod.lockclock.preference.Preferences" />
    </header>

</preference-headers>
